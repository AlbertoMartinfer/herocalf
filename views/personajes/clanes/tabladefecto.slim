.table-responsive
	table class="table table-striped table-bordered table-hover table-sm"
		thead class="thead-dark"
			tr
				th Personaje
				th colspan="5" Clanes
			- clanes.each do |pj|
				tr
					td == pj.first
					- pj.last.each do |clan|
						td
							a href="/clan/#{clan}"
								== clan